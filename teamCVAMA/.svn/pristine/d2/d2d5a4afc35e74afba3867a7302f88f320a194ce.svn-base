package com.cvama.clocal.controller;


import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;
import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

import com.cvama.clocal.service.ClocalService;

@RequestMapping("/clocal")
@Controller
public class ClocalController {
	
	@Qualifier("cl")
	@Autowired
	private ClocalService clocalService;
	
	
	@GetMapping("/list.do")
	public String list(Model model) {
		
		model.addAttribute("list", clocalService.list());
		
		return "clocal/list";
	}
	
	

	
}
